software =
{
	version = "{VERSION}";
	description = "Software update for SBT Instruments devices";
	zeus = {
		hardware-compatibility: ["1.0.0"];
		scripts: (
			{
				filename = "swupdate.sh";
				type = "shellscript";
			}
		);
		images: (
			{
				name = "system";
				filename = "system.img";
				device = "/dev/mmcblk0update";
			}
		);
	}
}

